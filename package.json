{
  "name": "@tcne/web-react-image-gallery",
  "version": "1.2.2",
  "description": "React Carousel, React Image gallery, React Slide Show component",
  "main": "./lib/Gallery",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "babel ./src --out-dir ./lib",
    "prepublish": "npm run build",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ThomasCookNE/react-image-gallery.git"
  },
  "keywords": [
    "react",
    "carousel",
    "react-component",
    "react-carousel",
    "react-slideshow",
    "react-gallery",
    "react carousel",
    "react slideshow",
    "react gallery",
    "image gallery",
    "image slider",
    "slideshow",
    "gallery",
    "slider"
  ],
  "author": "Team Web 1",
  "owner": "Team Web 1",
  "email": "xiao@linxtion.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ThomasCookNE/react-image-gallery/issues"
  },
  "homepage": "https://github.com/ThomasCookNE/react-image-gallery",
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-2"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.17.0",
    "babel-register": "^6.16.3",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^2.10.2",
    "eslint-plugin-react": "^5.1.1",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "imports-loader": "^0.6.5",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "resolve-url-loader": "^1.6.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0",
    "webpack-stats-plugin": "^0.1.3",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "keycode": "^2.1.7",
    "react-swipeable": "^3.3.1"
  }
}
